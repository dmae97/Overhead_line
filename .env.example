# 한전 전력데이터 개방포털 API Key (40자리)
KEPCO_API_KEY=

# 선택: 디버그 모드
DEBUG=false

# 선택: API 호출 타임아웃(초)
KEPCO_API_TIMEOUT_SECONDS=15

# 선택: API 호출 간 딜레이(초) - 서버 부하 방지
KEPCO_API_DELAY_SECONDS=0

# 선택: Selenium fallback (API 키 없을 때)
SELENIUM_HEADLESS=true
SELENIUM_PAGE_LOAD_TIMEOUT_SECONDS=40
SELENIUM_RESULT_TIMEOUT_SECONDS=30
